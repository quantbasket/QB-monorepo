{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@quantbasket/ui": ["../../packages/ui/src"],
      "@quantbasket/ui/*": ["../../packages/ui/src/*"],
      "@quantbasket/supabase-client": ["../../packages/supabase-client/src"],
      "@quantbasket/supabase-client/*": ["../../packages/supabase-client/src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
